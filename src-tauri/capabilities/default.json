{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "oauth:allow-start",
    "oauth:default",
    "oauth:allow-cancel",
    {
      "identifier": "opener:default",
      "allow": [
        {
          "url": "https://*.squareupsandbox.com"
        },
        {
          "url": "https://*.squareup.com"
        }
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.squareupsandbox.com"
        },
        {
          "url": "https://*.squareup.com"
        }
      ]
    },
    "opener:default"
  ]
}